CC="gcc"
CXX="g++"
ANNO_FLAGS="-specs=/usr/lib/rpm/redhat/redhat-annobin-cc1"
CFLAGS="-march=x86-64-v2 -mtune=generic -O2 -fno-lto -ffat-lto-objects -fexceptions -g -grecord-gcc-switches -pipe -Wall -Werror=format-security -Wp,-D_FORTIFY_SOURCE=2 -Wp,-D_GLIBCXX_ASSERTIONS -specs=/usr/lib/rpm/redhat/redhat-hardened-cc1 -fstack-protector-strong ${ANNO_FLAGS} -m64 -fasynchronous-unwind-tables -fstack-clash-protection -fcf-protection"
CXXFLAGS="${CFLAGS}"
FFLAGS="${CFLAGS} -I/usr/lib64/gfortran/modules"
FCFLAGS="${FFLAGS}"
LDFLAGS="-Wl,-z,relro -Wl,--as-needed -Wl,-z,now -specs=/usr/lib/rpm/redhat/redhat-hardened-ld ${ANNO_FLAGS}"
AR="gcc-ar"
NM="gcc-nm"
RANLIB="gcc-ranlib"
