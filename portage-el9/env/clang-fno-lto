CXXFLAGS="${CFLAGS}"
LDFLAGS="-Wl,-O2 -Wl,--as-needed -Wl,--hash-style=gnu -Wl,--sort-common"
AR="llvm-ar"
NM="llvm-nm"
RANLIB="llvm-ranlib"
